<?xml version="1.0" encoding="UTF-8"?>
	<addon id="skin.refless" version="1.06" name="Refless" provider-name="Hen">
	<requires>
		<import addon="xbmc.gui" version="5.0.1"/>
		<import addon="script.grab.fanart" version="0.13.0"/>
		<import addon="script.skinshortcuts" version="0.3.5"/>
		<import addon="service.library.data.provider" version="0.0.4"/>
	</requires>
	<extension point="xbmc.gui.skin" defaultthemename="Textures.xbt" effectslowdown="1.00" debugging="false">
